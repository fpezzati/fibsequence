{
  "AWSEBDockerrunVersion": "3",
  "containerDefinitions": [
    {
      "name": "fibclient",
      "image": "fpezzati/fibsequence_client",
      "hostname": "fibclient",
      "essential": false,
      "memory": 128
    },
    {
      "name": "fibworker",
      "image": "fpezzati/fibsequence_fibworker",
      "hostname": "fibworker",
      "essential": false,
      "memory": 128
    },
    {
      "name": "fibserver",
      "image": "fpezzati/fibsequence_server",
      "hostname": "fibserver",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "fpezzati/fibsequence_nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 9000
        }
      ],
      "links": [
        "fibclient", "fibserver"
      ],
      "memory": 128
    }
  ]
}
